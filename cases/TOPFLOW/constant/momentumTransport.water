FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      turbulenceProperties.water;
}

simulationType  RAS;

RAS
{
    model       mixtureKEpsilon;

    turbulence  on;
    printCoeffs on;

    mixtureKEpsilonCoeffs
    {
        Cp 0.25;
    }
}
